  "devDependencies": {
    "karma": "~0.10",
    "protractor": "~0.20.1",
    "http-server": "^0.6.1",
    "bower": "^1.3.1",
    "shelljs": "^0.2.6",
    "karma-junit-reporter": "^0.2.2",
    "gulp": "~3.6.2",
    "gulp-rename": "~1.2.0",
    "gulp-concat": "~2.2.0",
    "gulp-uglify": "~0.3.0",
    "gulp-clean": "~0.2.4",
    "gulp-minify-css": "~0.3.4",
    "gulp-autoprefixer": "0.0.7",
